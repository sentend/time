{
	"name": "backend",
	"type": "module",
	"version": "0.0.1",
	"main": "index.ts",
	"repository": "https://gitlab.com/timemator/timemator-webapp.git",
	"author": "Nikita Kungurcev",
	"license": "ISC",
	"private": true,
	"scripts": {
		"build": "tsc",
		"start": "node dist/index.js",
		"dev": "nodemon",
		"inspect": "nodemon --inspect src/index.ts",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"generate": "drizzle-kit generate:pg",
		"migrate": "ts-node src/db/migrate",
		"drop": "drizzle-kit drop",
		"studio": "drizzle-kit studio --port 8181 --verbose",
		"update": "node src/scripts/update.js",
		"db-push": "drizzle-kit push:pg",
		"db-seed": "ts-node -r tsconfig-paths/register src/db/seed/seed.ts"
	},
	"dependencies": {
		"@types/body-parser": "^1.19.3",
		"@types/cookie-parser": "^1.4.4",
		"body-parser": "^1.20.2",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"express-fileupload": "^1.4.1",
		"helmet": "^7.0.0",
		"lodash": "^4.17.21",
		"module-alias": "^2.2.3",
		"morgan": "^1.10.0",
		"nodemon": "^3.0.1",
		"passport": "^0.6.0",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.16.2"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.2.0",
		"@types/cors": "^2.8.14",
		"@types/express": "^4.17.18",
		"@types/express-fileupload": "^1.4.2",
		"@types/jest": "^29.5.5",
		"@types/morgan": "^1.9.5",
		"@types/node": "^20.6.5",
		"@types/pg": "^8.10.3",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"drizzle-kit": "^0.19.13",
		"drizzle-orm": "^0.32.0",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-jest": "^27.4.0",
		"jest": "^29.7.0",
		"pg": "^8.11.3",
		"prettier": "^3.0.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	}
}
